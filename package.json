{
  "name": "chess-llm-game",
  "version": "1.0.0",
  "description": "Chess game where two LLMs play against each other using OpenRouter API",
  "main": "dist/main.js",
  "scripts": {
    "start": "npm run build && node dist/main.js",
    "build": "tsc",
    "dev": "ts-node src/main.ts",
    "web": "npm run build && node dist/main.js web --port 3001",
    "play": "npm run build && node dist/main.js play",
    "interactive": "npm run build && node dist/main.js interactive",
    "models": "npm run build && node dist/main.js models",
    "test": "jest",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "chess",
    "llm",
    "openrouter",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "axios": "^1.5.0",
    "chalk": "^4.1.2",
    "chess.js": "^1.0.0-beta.8",
    "cli-table3": "^0.6.3",
    "commander": "^11.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "inquirer": "^8.2.6",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@types/chess.js": "^0.13.5",
    "@types/inquirer": "^8.2.6",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "autoprefixer": "^10.4.21",
    "jest": "^29.6.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.0"
  }
}
